{
  "name": "json-compare-vue",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name json-compare-vue src/index.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "ace-builds": "^1.43.2",
    "codemirror": "^5.65.9",
    "diff-match-patch": "^1.0.5",
    "file-saver": "^2.0.5",
    "jmespath": "^0.16.0",
    "jsondiffpatch": "^0.7.3",
    "jsoneditor": "^10.3.0",
    "jsonrepair": "^3.13.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "files": [
    "dist",
    "src/assets/jsondiffpatch-html.css",
    "README.md",
    "README.zh-CN.md",
    "LICENSE"
  ],
  "main": "dist/json-compare-vue.common.js",
  "module": "dist/json-compare-vue.esm.js",
  "unpkg": "dist/json-compare-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/json-compare-vue.esm.js",
      "require": "./dist/json-compare-vue.common.js"
    }
  },
  "keywords": [
    "json",
    "diff",
    "jsoneditor",
    "vue",
    "vue3",
    "compare"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
